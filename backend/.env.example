PORT=3001
NODE_ENV=development

# Database Configuration for pgAdmin users
# Replace 'postgres' with your actual PostgreSQL username
# Replace 'your_password' with your actual PostgreSQL password
# Default port is usually 5432
DATABASE_URL=postgresql://postgres:your_password@localhost:5432/bookit_db

# CORS - Frontend URL
FRONTEND_URL=http://localhost:5173

# Example for different configurations:
# For custom username: postgresql://myuser:mypass@localhost:5432/bookit_db
# For different port: postgresql://postgres:password@localhost:5433/bookit_db
# For remote database: postgresql://user:pass@your-db-host:5432/bookit_db